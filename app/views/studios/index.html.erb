<h1> All Studios</h1>

<% @studios.each do |studio| %>
  <section id='studio-<%=studio.id%>'>
    Studio Name:
    <%=studio.name%><br><br>
    Movies: <br>
    <%studio.movies.each do |movie|%>
      <%= link_to "#{movie.name}", "/movies/#{movie.id}"%><br>
    <%end%><br><br><br>
  </section>
<%end%><br><br><br>
